@fontName: 'Font Awesome 5 Free';
@fontFileName: 'fa-solid-900';
@src:
  url("@{fontPath}/@{fontFileName}.eot?#iefix") format('embedded-opentype'),
  url("@{fontPath}/@{fontFileName}.woff2") format('woff2'),
  url("@{fontPath}/@{fontFileName}.woff") format('woff'),
  url("@{fontPath}/@{fontFileName}.ttf") format('truetype'),
  url("@{fontPath}/@{fontFileName}.svg#icons") format('svg');

@distanceFromText: 0.25em;
